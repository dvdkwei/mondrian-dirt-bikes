<Schema name="HyperSchema">
    <Cube name="HyperCube@DirtBikes" cache="true" enabled="true">
        <View alias="JOINS1">
            <SQL dialect="generic">
                select * from wi2_acu008_fact_table
                join wi2_acu008_customer w2a008c on wi2_acu008_fact_table.FACT_C_ID = w2a008c.C_ID
                join wi2_acu008_product w2a008p on wi2_acu008_fact_table.FACT_P_ID = w2a008p.P_ID
                join wi2_acu008_time on FACT_T_ID = T_ID
            </SQL>
        </View>

