<Schema name="HyperSchema">
    <Cube name="HyperCube@DirtBikes" cache="true" enabled="true">

        <Table name="WI2_ACU008_FACT_TABLE">
                <AggName name="AGG_2_WI2_ACU008_FACT_TABLE" ChooseByVolume="false">
                <AggFactCount column="FACT_COUNT"/>
                <AggMeasure name="[Measures].[REVENUE]" column="sum_revenue"/>
                <AggMeasure name="[Measures].[DISCOUNT]" column="sum_discount"/>
                <AggMeasure name="[Measures].[COSTOFGOODSSOLD]" column="sum_cogs"/>
                <AggMeasure name="[Measures].[SALESQUANTITY]" column="sum_salesquantity"/>
                <AggMeasure name="[Measures].[NOOFSALESORDERS]" column="sum_noofsalesorders"/>

                <AggLevel name="[TIME].[YEAR]" column="TIME_YEAR"/>
                <AggLevel name="[TIME].[MONTH]" column="TIME_MONTH"/>

                <AggLevel name="[PRODUCT].[DIVISION]" column="PRODUCT_P_DIVISION"/>
                <AggLevel name="[PRODUCT].[PRODUCT_CATEGORY]" column="PRODUCT_P_CATEGORYID"/>

                <AggLevel name="[CUSTOMER].[CUSTOMER]" column="CUSTOMER_C_ID" collapsed="false"/>
            </AggName>
        </Table>

        <Dimension name="TIME" foreignKey="FACT_T_ID">
            <Hierarchy primaryKey="T_ID" hasAll="true">
                <Table name="WI2_ACU008_TIME" schema="ACU008"/>
                <Level name="YEAR" column="T_YEAR"/>
                <Level name="MONTH" column="T_MONTH"/>
                <Level name="DAY" column="T_DAY">
                    <Property name="TIME" column="T_ID"/>
                </Level>

            </Hierarchy>
        </Dimension>

        <Dimension name="CUSTOMER" foreignKey="FACT_C_ID">
            <Hierarchy primaryKey="C_ID" hasAll="true">
                <Table name="WI2_ACU008_CUSTOMER" schema="ACU008"/>
                <Level name="COUNTRY" column="C_LAND"/>
                <Level name="SALESORGCURRENT" column="C_SALESORGCURRENT">
                    <Property name="SALESORGHISTORIC" column="C_SALESORGHISTORIC"/>
                </Level>

                <Level name="CITY" column="C_CITY"/>

                <Level name="CUSTOMER" column="C_ID" nameColumn="C_DESC" uniqueMembers="true">
                    <Property name="VALID_FROM" column="VALIDFROM"/>
                    <Property name="VALID_TO" column="VALIDTO"/>
                    <Property name="ONGOING" column="ONGOING"/>
                </Level>
            </Hierarchy>
        </Dimension>

        <Dimension name="PRODUCT" foreignKey="FACT_P_ID">
            <Hierarchy primaryKey="P_ID" hasAll="true">
                <Table name="WI2_ACU008_PRODUCT" schema="ACU008"/>
                <Level name="DIVISION" column="P_DIVISION"/>
                <Level name="PRODUCT_CATEGORY" column="P_CATEGORYID" nameColumn="P_CATEGORY"/>

                <Level name="PRODUCT" column="P_ID" nameColumn="P_DESC" uniqueMembers="true">
                    <Property name="SALES_PRICE" column="P_SALESPRICE"/>
                    <Property name="PRODUCTION_PRICE" column="P_PRODUCTIONPRICE"/>
                </Level>
            </Hierarchy>
        </Dimension>

        <Measure name="REVENUE" column="REVENUE" aggregator="sum"/>
        <Measure name="DISCOUNT" column="DISCOUNT" aggregator="sum"/>
        <Measure name="COSTOFGOODSSOLD" column="COGS" aggregator="sum"/>
        <Measure name="SALESQUANTITY" column="SALESQUANTITY" aggregator="sum"/>
        <Measure name="NOOFSALESORDERS" column="NOOFSALESORDERS" aggregator="sum"/>
    </Cube>
</Schema>

